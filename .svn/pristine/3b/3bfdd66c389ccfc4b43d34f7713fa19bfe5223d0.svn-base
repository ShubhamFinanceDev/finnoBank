	<script type="text/javascript">
		$(function() {

			$('.js-basic-example').DataTable({
				responsive : false,
				"pageLength" : 5,
				"iDisplayLength" : 5

			});

		});

		function editbranch(id, name,location,state,state) {
			$("#branchNameid").val(id);
			if (name != null) {
				$("#branchName").val(name);
				$("#location").val(location);
				$("#branchCode").val(state);
				
				$("#state").val(state);
				
			} else {
				$('#formbranchsubmit').attr('action', "/deletebranch").submit();
			}
		}

		function editdepartment(id, name) {
			$("#departmentNameid").val(id);
			if (name != null) {
				$("#departmentName").val(name);
			} else {
				$('#formdepartmentsubmit').attr('action', "/deletedepartment")
						.submit();
			}
		}
		function edititem(id, name) {
			$("#itemNameid").val(id);
			if (name != null) {
				$("#itemName").val(name);
			} else {
				$('#formitemsubmit').attr('action', "/deleteitem").submit();
			}
		}

		function editcourier(id, name) {
			$("#courierNameid").val(id);
			if (name != null) {
				$("#courierName").val(name);
			} else {
				$('#formcouriersubmit').attr('action', "/deletecourier")
						.submit();
			}
		}
	</script>
